n       history.push('/login')\n     } else {\n       if (!user || !user.name || success) {\n         dispatch({ type: USER_UPDATE_PROFILE_RESET })\n         dispatch(getUserDetails('profile'))\n         dispatch(listMyOrders())\n       } else {\n         setName(user.name)\n         setEmail(user.email)\n       }\n     }\n   }, [dispatch, history, userInfo, user])\n\n   const submitHandler = (e) => {\n     e.preventDefault()\n     if (password !== confirmPassword) {\n       setMessage('Passwords do not match')\n     } else {\n       dispatch(updateUserProfile({ id: user._id, name, email, password }))\n     }\n}\n\nreturn (\n     <Row>\n       <Col md={3}>\n         <h2>User Profile</h2>\n         {message && <Message variant='danger'>{message}</Message>}\n         {error && <Message variant='danger'>{error}</Message>}\n         {success && <Message variant='success'>Profile Updated</Message>}\n         {loading && <Loader />}\n         <Form onSubmit={submitHandler}>\n           <Form.Group controlId='name'>\n             <Form.Label>Name</Form.Label>\n             <Form.Control\n               type='name'\n               placeholder='Enter name'\n               value={name}\n               onChange={(e) => setName(e.target.value)}\n             ></Form.Control>\n           </Form.Group>\n\n           <Form.Group controlId='email'>\n             <Form.Label>Email Address</Form.Label>\n             <Form.Control\n               type='email'\n               placeholder='Enter email'\n               value={email}\n               onChange={(e) => setEmail(e.target.value)}\n             ></Form.Control>\n           </Form.Group>\n\n           <Form.Group controlId='password'>\n             <Form.Label>New Password</Form.Label>\n             <Form.Control\n               type='password'\n               placeholder='Enter password'\n               value={password}\n               onChange={(e) => setPassword(e.target.value)}\n             ></Form.Control>\n           </Form.Group>\n\n           <Form.Group controlId='confirmPassword'>\n             <Form.Label>Confirm Password</Form.Label>\n             <Form.Control\n               type='password'\n               placeholder='Confirm password'\n               value={confirmPassword}\n               onChange={(e) => setConfirmPassword(e.target.value)}\n             ></Form.Control>\n           </Form.Group>\n\n           <Button type='submit' variant='primary'>\n             Update\n           </Button>\n         </Form>\n       </Col>\n       <Col md={9}>\n         <h2>My Orders</h2>\n         {loadingOrders ? (\n           <Loader />\n         ) : errorOrders ? (\n           <Message variant='danger'>{errorOrders}</Message>\n         ) : (\n           <Table striped bordered hover responsive className='table-sm'>\n             <thead>\n               <tr>\n                 <th>ID</th>\n                 <th>DATE</th>\n                 <th>TOTAL</th>\n                 <th>PAID</th>\n                 <th>DELIVERED</th>\n                 <th></th>\n               </tr>\n             </thead>\n             <tbody>\n               {orders.map((order) => (\n                 <tr key={order._id}>\n                   <td>{order._id}</td>\n                   <td>{order.createdAt.substring(0, 10)}</td>\n                   <td>{order.totalPrice}</td>\n                   <td>\n                     {order.isPaid ? (\n                       order.paidAt.substring(0, 10)\n                     ) : (\n                       <i className='fas fa-times' style={{ color: 'red' }}></i>\n                     )}\n                   </td>\n                   <td>\n                     {order.isDelivered ? (\n                       order.deliveredAt.substring(0, 10)\n                     ) : (\n                       <i className='fas fa-times' style={{ color: 'red' }}></i>\n                     )}\n                   </td>\n                   <td>\n                     <LinkContainer to={`/order/${order._id}`}>\n                       <Button className='btn-sm' variant='light'>\n                         Details\n                       </Button>\n                     </LinkContainer>\n                   </td>\n                 </tr>\n               ))}\n             </tbody>\n           </Table>\n         )}\n       </Col>\n     </Row>\n   )\n}\n\nexport default ProfileScreen","/Users/peter/OneDrive/proshop/frontend/src/screens/ShippingScreen.js",[],"/Users/peter/OneDrive/proshop/frontend/src/screens/PlaceOrderScreen.js",[],"/Users/peter/OneDrive/proshop/frontend/src/screens/PaymentScreen.js",[],"/Users/peter/OneDrive/proshop/frontend/src/components/CheckoutSteps.js",[],"/Users/peter/OneDrive/proshop/frontend/src/actions/orderActions.js",[],"/Users/peter/OneDrive/proshop/frontend/src/constants/orderConstants.js",[],"/Users/peter/OneDrive/proshop/frontend/src/reducers/orderReducers.js",[],"/Users/peter/OneDrive/proshop/frontend/src/screens/UserEditScreen.js",[],"/Users/peter/OneDrive/proshop/frontend/src/screens/UserListScreen.js",[],"/Users/peter/OneDrive/proshop/frontend/src/screens/ProductListScreen.js",[],"/Users/peter/OneDrive/proshop/frontend/src/screens/ProductEditScreen.js",[],{"ruleId":"164","replacedBy":"165"},{"ruleId":"166","replacedBy":"167"},{"ruleId":"168","severity":1,"message":"169","line":18,"column":8,"nodeType":"170","messageId":"171","endLine":18,"endColumn":25},{"ruleId":"172","severity":2,"message":"173","line":38,"column":56,"nodeType":"170","messageId":"174","endLine":38,"endColumn":74},{"ruleId":"164","replacedBy":"175"},{"ruleId":"166","replacedBy":"176"},{"ruleId":"177","severity":1,"message":"178","line":45,"column":7,"nodeType":"179","endLine":45,"endColumn":42,"suggestions":"180"},"no-native-reassign",["181"],"no-negated-in-lhs",["182"],"no-unused-vars","'ProductListScreen' is defined but never used.","Identifier","unusedVar","no-undef","'ProductsListScreen' is not defined.","undef",["181"],["182"],"react-hooks/exhaustive-deps","React Hook